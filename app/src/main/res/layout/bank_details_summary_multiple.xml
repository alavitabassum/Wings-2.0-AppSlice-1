            android:id="@+id/date4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text = "Date: 2019-09-26"
            android:textSize="12dp"/>
        <TextView
            android:id="@+id/bankName4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text = "Bank Name: Eastern Bank Limited."
            android:layout_below="@+id/date4"
            android:textSize="12dp"/>
        <TextView
            android:id="@+id/depositeSlip4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text = "Slip: Abcd-1234"
            android:layout_below="@+id/bankName4"
            android:textSize="12dp"/>
        <TextView
            android:id="@+id/depositedAmount4"
            android:layout_width="wrap_content"
