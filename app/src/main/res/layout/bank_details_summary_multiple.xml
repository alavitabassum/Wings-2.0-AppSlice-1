<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.paperflywings.user.paperflyv0.DeliveryApp.DeliverySupervisor.BankFragmentContent.BankDepositBySupervisor">

    <TextView
        android:id="@+id/total_cashAmt1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:visibility="gone"
        android:text="Total Cash: 4 Taka"/>
    <TextView
        android:id="@+id/total_cashAmt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/total_cashAmt1"
        android:visibility="gone"
        android:layout_centerHorizontal="true"
        android:text="Submitting Total Cash: 4 Taka"/>

    <!--1st slip-->
    <TextView
        android:id="@+id/text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text = "Slip-1"
        android:layout_marginTop="10dp"
        android:paddingTop="10dp"
        android:visibility="gone"
        android:textSize="12dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/total_cashAmt2"
        android:textStyle="bold"/>

    <RelativeLayout
        android:id="@+id/text_holder1"
        android:layout_below="@+id/text1"
        android:layout_width="wrap_content"
        android:layout_centerHorizontal="true"
        android:visibility="gone"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/date1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text = "Date: 2019-09-26"
            android:textSize="12dp"/>
        <TextView
            android:id="@+id/bankName1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text = "Bank Name: Eastern Bank Limited."
            android:layout_below="@+id/date1"
            android:textSize="12dp"/>
        <TextView
            android:id="@+id/depositeSlip1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text = "Slip: Abcd-1234"
            android:layout_below="@+id/bankName1"
            android:textSize="12dp"/>
        <TextView
            android:id="@+id/depositedAmount1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text = "Deposite: 12000 Taka"
            android:layout_below="@+id/depositeSlip1"
            android:textSize="14dp"
            android:textStyle="bold"/>
    </RelativeLayout>

    <!--2nd slip-->
    <TextView
        android:id="@+id/text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text = "Slip-2"
        android:layout_marginTop="10dp"
        android:visibility="gone"
        android:textSize="12dp"
        android:paddingTop="10dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/text_holder1"
        android:textStyle="bold"/>

    <RelativeLayout
        android:id="@+id/text_holder2"
        android:layout_below="@+id/text2"
        android:layout_width="wrap_content"
        android:visibility="gone"
        android:layout_centerHorizontal="true"
